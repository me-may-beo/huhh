                Save("KillNhoc", v)
            end
        })
    Tabs.V4:AddToggle("Auto Farm",
        {
            Title = "Auto Farm Quest Acient One",
            Default = Settingss.QAO,
            Callback = function(v)
                Config.QAO = v
                Save("QAO", v)
            end
        })

