        if isnetworkowner2(part) then
            return isnetworkowner2(part)
        end
        return part.ReceiveAge == 0 and GetNearestPlayer(part.Position)
    end
else
    warn("already isnetworkowner ðŸ˜ŽðŸ˜ŽðŸ˜Ž") -- bruh
